{
  "all": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/__tests__/**",
    "dist/**",
    "node_modules/**"
  ],
  "reporter": [
    "text",
    "lcov",
    "html"
  ],
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 75
}
